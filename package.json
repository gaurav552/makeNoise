{
  "name": "makenoise",
  "version": "1.0.0",
  "description": "A framework-agnostic global media player library for Single Page Applications",
  "type": "module",
  "main": "./dist/makenoise.cjs",
  "module": "./dist/makenoise.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/makenoise.js",
      "require": "./dist/makenoise.cjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs",
      "types": "./dist/adapters/react/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.js",
      "require": "./dist/vue.cjs",
      "types": "./dist/adapters/vue/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "audio",
    "player",
    "media",
    "music",
    "spa",
    "persistent",
    "framework-agnostic",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/makenoise.git"
  },
  "homepage": "https://github.com/yourusername/makenoise#readme",
  "bugs": {
    "url": "https://github.com/yourusername/makenoise/issues"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.8",
    "@vitest/ui": "^1.0.0",
    "@vue/test-utils": "^2.4.6",
    "fast-check": "^3.15.0",
    "jsdom": "^23.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue": "^3.5.26"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}